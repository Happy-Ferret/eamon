{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "forin": false,
    "jsx-no-lambda": false,
    "member-access": false,
    "member-ordering": false,
    "no-console": false,
    "no-debugger": false,
    "no-empty": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "prefer-const": false,
    "variable-name": false
  }
}
